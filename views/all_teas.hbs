 <div id="content">
    <div class="a" id="map">
      <div>
        
      </div>
      {{#if status}}
      <div id="addTea">
       <a href=""><button id="b" class="sing more">Add new tea</button></a> 
      </div>
      {{/if}}
    </div>
    <div class="a" id="comments">
      {{#each latestComments}}
      <div class="post">
        <div>
          <a href="http://localhost:3000/all_teas/{{this.Tea.id}}"><img class="foto" src="{{this.Tea.image}}" alt=""></a>
        </div>
        <div class="com">
          <div class="tea">
          {{this.Tea.tea_name}}
          </div>
         <div class="text">
          {{this.content}}
          </div>
          <div class="autor">
          {{this.User.login}}
          </div>
        </div>
      </div>
      {{/each}}
    </div>
  </div>
</body>
