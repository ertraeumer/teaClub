 <div id="content">
    <div class="a" id="map">
      <div>
        
      </div>
      {{!-- {{#if}} --}}
      <div>
        <div id="addTea">
       <a class="more" href=""><div id="add" class="more">Add new tea</div></a> 
      </div>
      </div>
      {{!-- {{/if}} --}}
    </div>
    <div class="a" id="comments">
      {{#each latestComments}}
      <div class="post">
        <div>
          <a href="http://localhost:3000/all_teas/{{this.Tea.id}}"><img class="foto" src="{{this.Tea.image}}" alt="https://besttea.ru/images/watermarked/1/detailed/11/ba-164.jpg"></a>
        </div>
        <div class="com">
          <div class="tea">
          {{this.Tea.tea_name}}
          </div>
         <div class="text">
          {{this.content}}
          </div>
          <div class="autor">
          {{this.User.login}}
          </div>
        </div>
      </div>
      {{/each}}
    </div>
  </div>
</body>
